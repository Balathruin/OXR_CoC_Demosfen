spawn_num = {+bar_deactivate_radar_done} 1, 0

[spawn_freedom@veteran]
spawn_squads =  freedom_sim_squad_advanced, freedom_sim_squad_advanced, freedom_sim_squad_veteran
spawn_num = {+bar_deactivate_radar_done} 1, 0