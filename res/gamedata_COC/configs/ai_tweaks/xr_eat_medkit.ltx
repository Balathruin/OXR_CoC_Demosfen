[plugin]
enable = true
refused_names = none
in_combat = true					;Can use in combat
out_combat = true					;Can use outside combat
medkit_health = 50				;Percent of health before npc uses medkit
bandage_bleeding = 0.15
medkits = stimpack,stimpack_army,stimpack_scientic,medkit,medkit_army,medkit_scientic		; Usable sections
bandages = stimpack,stimpack_army,stimpack_scientic,bandage
excluded_communities = zombied

[novice]
medkit_delay = 12400,14600				;Min-Max delay before eating
bandage_delay = 12400,14600

[experienced]
medkit_delay = 10200,10900				;Min-Max delay before eating
bandage_delay = 10200,10900

[veteran]
medkit_delay = 8800,9400				;Min-Max delay before eating
bandage_delay = 8800,9400

[master]
medkit_delay = 7100,8600				;Min-Max delay before eating
bandage_delay = 7100,8600

